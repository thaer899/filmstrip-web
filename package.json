{
  "name": "filmstrip-web",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/yenbekbay/filmstrip-web",
  "repository": "yenbekbay/filmstrip-web",
  "license": "GPL-3.0",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": "6.x.x"
  },
  "scripts": {
    "deploy": "sh ./scripts/deploy.sh",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate-tachyons": "babel-node scripts/generate-tachyons.js",
    "test": "npm-run-all --parallel typecheck lint",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "flow check .",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm test"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!.next/**",
      "!flow-typed/**"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "jsdom",
    "testPathDirs": [
      "<rootDir>/components",
      "<rootDir>/data",
      "<rootDir>/hocs",
      "<rootDir>/pages"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "dependencies": {
    "apollo-client": "^0.5.6",
    "graphql-tag": "^0.1.17",
    "iso-639-1": "^1.2.3",
    "lodash": "^4.17.2",
    "next": "^1.1.2",
    "react": "^15.4.1",
    "react-apollo": "^0.6.0",
    "react-cookie": "^0.4.8",
    "react-dom": "^15.4.1",
    "react-modal": "^1.5.2",
    "react-redux": "^4.4.6",
    "react-youtube": "^7.2.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "@types/css": "^0.0.30",
    "@types/graphql": "^0.8.2",
    "@types/isomorphic-fetch": "^0.0.31",
    "@types/jest": "^16.0.0",
    "@types/lodash": "^4.14.40",
    "@types/react": "^0.14.50",
    "@types/react-modal": "^1.3.6",
    "@types/react-redux": "^4.4.33",
    "@types/redux": "^3.6.31",
    "babel-cli": "^6.18.0",
    "babel-jest": "^17.0.2",
    "commitizen": "^2.8.6",
    "css": "^2.2.1",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "~3.9.1",
    "eslint-config-anvilabs": "^2.0.1",
    "flow-bin": "^0.35.0",
    "graphql": "^0.8.2",
    "husky": "^0.11.9",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^17.0.3",
    "native-css": "^1.2.5",
    "npm-run-all": "^3.1.1",
    "validate-commit-msg": "^2.8.2"
  }
}
